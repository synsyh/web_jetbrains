<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>examlist</servlet-name>
        <servlet-class>servlet.examlistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>examlist</servlet-name>
        <url-pattern>/examlist</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>paper</servlet-name>
        <servlet-class>servlet.paperServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>paper</servlet-name>
        <url-pattern>/paper</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>stu_information</servlet-name>
    <servlet-class>servlet.stu_informationServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>stu_information</servlet-name>
        <url-pattern>/stu_information</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>stu_sys</servlet-name>
        <servlet-class>servlet.stu_sysServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>stu_sys</servlet-name>
        <url-pattern>/stu_sys</url-pattern>
    </servlet-mapping>
</web-app>